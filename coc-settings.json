{
  "coc.preferences.formatOnSaveFiletypes": [
    "yaml",
    "json",
    "markdown",
    "pandoc",
    "xml",
    "toml",
    "python",
    "rust",
    "go"
  ],
  "prettier.proseWrap": "always",
  "codeLens.enable": true,
  "python.formatting.provider": "black",
  "rust-analyzer.cargo.autoreload": true,
  "go.goplsOptions": {
    "completeUnimported": true
  }
}
